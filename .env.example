## PostgreSQL (service: db)
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflow
POSTGRES_DB=airflow
POSTGRES_PORT=5432

## Airflow (created by airflow-init)
AIRFLOW_ADMIN_USERNAME=admin
AIRFLOW_ADMIN_PASSWORD=admin

## Linux user id for file permissions (optional)
# AIRFLOW_UID=50000

## Airflow security: Fernet key (required)
# Generate one with:
#   python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AIRFLOW_FERNET_KEY=REPLACE_ME_WITH_A_FERNET_KEY
